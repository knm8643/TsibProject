<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.trys.tsibproject.mapper.springTestMapper">
    <insert id="insertTestMapper">
        insert into TsibProject.test (mem_name) values (#{userName});
    </insert>

    <!-- PostPage (test) -->
    <insert id="postSaveMapper">
        insert into TsibProject.test (post_title, post_media, post_tag, post_content) values (#{postTitle}, #{postMedia}, #{postTag}, #{postContent});
    </insert>
</mapper>



